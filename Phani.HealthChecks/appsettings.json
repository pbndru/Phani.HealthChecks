{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Phani.DeveloperApi",
        "Uri": "https://localhost:5001/health-checks"
      },
      {
        "Name": "Phani.TesterApi",
        "Uri": "https://localhost:5002/health-checks"
      }
    ],
    "EvaluationTimeInSeconds": 10
  },
  "AllowedHosts": "*"
}